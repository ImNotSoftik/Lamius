--- /dev/null
+++ b/src/main/java/me/core/lamius/Lamius/event/server/WorldTickEndEvent.java
@@ -0,0 +1,44 @@
+package me.core.lamius.Lamius.event.server;
+
+import org.bukkit.World;
+import org.bukkit.event.HandlerList;
+import org.bukkit.event.server.ServerEvent;
+
+public class WorldTickEndEvent extends ServerEvent {
+    private static final HandlerList handlers = new HandlerList();
+
+    private World world;
+    private int tickNumber;
+    private double tickDuration;
+
+    public WorldTickEndEvent() {
+        super(false);
+    }
+
+    public World getWorld() {
+        return world;
+    }
+
+    public int getTickNumber() {
+        return tickNumber;
+    }
+
+    public double getTickDuration() {
+        return tickDuration;
+    }
+
+    public void setData(World world, int tickNumber, double tickDuration) {
+        this.world = world;
+        this.tickNumber = tickNumber;
+        this.tickDuration = tickDuration;
+    }
+
+    @Override
+    public HandlerList getHandlers() {
+        return handlers;
+    }
+
+    public static HandlerList getHandlerList() {
+        return handlers;
+    }
+}
